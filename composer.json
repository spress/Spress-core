{
    "name": "spress/spress-core",
    "description": "Static site generator with blogs support",
    "keywords": ["cms","blog","web","markdown","twig","silex","html"],
    "license": "MIT",
    "homepage": "http://spress.yosymfony.com",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "twig/twig": ">=1.8,<2.0-dev",
        "pimple/pimple": "~3.0",
        "symfony/yaml": "2.7.*",
        "symfony/finder": "2.7.*",
        "symfony/filesystem": "2.7.*",
        "symfony/event-dispatcher": "2.7.*",
        "michelf/php-markdown": "1.5.*",
        "dflydev/embedded-composer": "1.0.*@dev",
        "yosymfony/config-loader": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "Yosymfony\\Spress\\Core\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
